<div class="row">
  <span class="small-12 column">
    <h1>Edit <%= @food_element.name %></h1>
  </span>
  <%= form_for @food_element.becomes(FoodElement) do |f| %>
    <%= render 'food_element_form_fields', f: f %>
  
    <div class="small-12 column">
      <%= f.submit 'Update Food Element', class: 'button' %>
    </div>
  <% end %>
</div>
